{
  "name": "hydra-sdk-auth0-consent",
  "version": "0.0.1",
  "scripts": {
    "build": "babel src --out-dir ./build --copy-files --ignore .test.js",
    "start": "node --harmony ./build/bin/www.js",
    "dev": "nodemon --exec babel-node ./src/bin/www.js",
    "watch:js": "chokidar \"+src/**\" --initial",
    "test": "jest",
    "test:watch": "jest --watch",
    "flow": "flow check",
    "format": "prettier --single-quote --parser flow --no-semi --write \"src/**/*.js\"",
    "flow:generate": "flow-typed install"
  },
  "dependencies": {
    "auth0": "~2.8.0",
    "body-parser": "~1.13.2",
    "connect-flash": "~0.1.1",
    "cookie-parser": "~1.3.5",
    "csurf": "~1.9.0",
    "debug": "~2.2.0",
    "detect-port": "~1.2.1",
    "dotenv": "~4.0.0",
    "express": "~4.13.1",
    "express-session": "~1.11.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "~7.1.9",
    "jwk-to-pem": "~1.2.5",
    "jwt-decode": "~2.2.0",
    "morgan": "~1.6.1",
    "ory-hydra-sdk": "~0.10.0-alpha.7",
    "passport": "~0.3.0",
    "passport-auth0": "~0.6.0",
    "passport-local": "~1.0.0",
    "promise-retry": "^1.1.1",
    "retry": "~0.10.1",
    "serve-favicon": "~2.3.0",
    "simple-oauth2": "~1.0.0",
    "superagent": "~2.3.0",
    "url-join": "~2.0.2",
    "uuid": "~3.1.0",
    "validator": "~8.2.0",
    "winston": "~2.3.1"
  },
  "devDependencies": {
    "babel-cli": "~6.26.0",
    "babel-preset-env": "~1.6.0",
    "babel-preset-flow": "~6.23.0",
    "babel-preset-stage-0": "~6.24.1",
    "chokidar-cli": "~1.2.0",
    "eslint": "~3.5.0",
    "flow-bin": "0.58.0",
    "flow-typed": "2.2.3",
    "jest": "~15.1.1",
    "nock": "~8.0.0",
    "nodemon": "~1.11.0",
    "prettier": "^1.7.4"
  },
  "jest": {
    "roots": [
      "./src"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "flow-typed",
      "*.test.js",
      "src/public",
      "build",
      "src/views"
    ]
  }
}
