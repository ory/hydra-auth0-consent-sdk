{
  "name": "hydra-consent-sdk",
  "version": "0.0.0",
  "main": "./lib/index.js",
  "scripts": {
    "build": "babel src --out-dir ./lib --copy-files --ignore .test.js",
    "build:watch": "babel src --watch --out-dir ./lib --copy-files --ignore .test.js",
    "start": "node --harmony ./example/bin/www.js",
    "dev": "nodemon --exec babel-node ./src/bin/www.js",
    "test": "cross-env HYDRA_CLIENT_ID=client HYDRA_CLIENT_SECRET=secret HYDRA_URL=http://hydra.localhost jest",
    "test:watch": "npm run test -- --watch",
    "flow": "flow check",
    "format": "prettier --single-quote --parser flow --no-semi --write \"src/**/*.js\"",
    "flow:generate": "flow-typed install"
  },
  "dependencies": {
    "auth0": "~2.8.0",
    "body-parser": "^1.18.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "express-session": "^1.15.6",
    "ory-hydra-sdk": "0.10.0-alpha.20",
    "passport": "^0.4.0",
    "passport-auth0": "~0.6.0",
    "promise-retry": "^1.1.1",
    "simple-oauth2": "~1.0.0",
    "url-join": "~2.0.2",
    "uuid": "^3.1.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-cli": "~6.26.0",
    "babel-preset-env": "~1.6.0",
    "babel-preset-flow": "~6.23.0",
    "babel-preset-stage-0": "~6.24.1",
    "csurf": "^1.9.0",
    "detect-port": "^1.2.2",
    "express": "^4.16.2",
    "flow-bin": "0.58.0",
    "flow-typed": "2.2.3",
    "jade": "^1.11.0",
    "jest": "~15.1.1",
    "lerna": "^2.0.0",
    "nock": "~8.0.0",
    "nodemon": "~1.11.0",
    "prettier": "^1.7.4",
    "superagent": "^3.8.1"
  },
  "jest": {
    "rootDir": "./src"
  },
  "nodemonConfig": {
    "ignore": [
      "flow-typed",
      "*.test.js",
      "src/public",
      "build",
      "src/views"
    ]
  }
}
